<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@android:color/transparent" />
        </shape>
    </item>
    <item
        android:right="10dp"
        android:left="10dp"
        >
        <shape>
            <stroke
                android:width="@dimen/_10sdp"
                android:color="#18AB33" />
            <solid android:color="@color/transparent" />
        </shape>
    </item>
    <item
        android:bottom="4dp">
        <shape>
            <solid android:color="@color/color_bottom_nav_bg" />
        </shape>

    </item>
</layer-list>